"*":
  "advanced-open-file":
    createDirectories: true
    createFileInstantly: true
    fuzzyMatch: true
  "atom-alignment": {}
  "atom-css-comb": {}
  "atom-csscomb": {}
  "atom-fuzzy-grep":
    inputThrottle: 200
  "atom-terminal-panel":
    WindowHeight: 450
    statusBarText: "%(dynamic) %(username) [%(hours):%(minutes):%(seconds)]"
  "atom-ternjs":
    displayAboveSnippets: true
    useSnippets: true
    useSnippetsAndFunction: true
  blame:
    defaultWidth: 280
  core:
    disabledPackages: [
      "autocomplete-paths"
      "react"
      "linter-csslint"
    ]
    ignoredNames: [
      ".git"
      ".DS_Store"
      "._*"
      "Thumbs.db"
      "node_modules"
      "target"
      ".atom"
    ]
    openEmptyEditorOnStart: false
    packagesWithKeymapsDisabled: [
      "atom-terminal-panel"
      "git-time-machine"
      "git-blame"
    ]
    projectHome: "D:\\repo"
    themes: [
      "one-dark-ui"
      "base16-tomorrow-dark-theme"
    ]
  "css-comb": {}
  editor:
    fontFamily: "Hack"
    fontSize: 16
    invisibles: {}
    showIndentGuide: true
    showInvisibles: true
    softWrap: true
    tabType: "soft"
  "file-watcher":
    promptWhenFileHasChangedOnDisk: false
  "git-blame":
    columnWidth: 270
  "git-plus": {}
  "highlight-selected":
    timeout: 100
  "js-hyperclick": {}
  linter:
    errorPanelHeight: 26
  "linter-eslint":
    disableWhenNoEslintrcFileInPath: true
  "merge-conflicts": {}
  minimap:
    plugins:
      "find-and-replace": true
      "find-and-replaceDecorationsZIndex": 0
      "git-diff": true
      "git-diffDecorationsZIndex": 0
      "split-diff": true
      "split-diffDecorationsZIndex": 0
  "mocha-test-runner":
    nodeBinaryPath: "C:\\nodejs\\node.exe"
  nuclide:
    remoteProjectsConfig: []
  "nuclide-file-tree": {}
  "nuclide-health":
    showCpu: true
    showMemory: true
  pigments: {}
  react: {}
  "save-session":
    dataSaveFolder: "C:\\Users\\edloidas\\.atom\\packages\\save-session\\projects"
    skipSavePrompt: false
  "smart-tab-name": {}
  "split-diff":
    diffWords: true
    ignoreWhitespace: true
    leftEditorColor: "red"
    rightEditorColor: "green"
    syncHorizontalScroll: true
  "symbols-tree-view": {}
  tabs:
    enableVcsColoring: true
  tasks: {}
  "tool-bar": {}
  welcome:
    showOnStartup: false
".jade.source":
  editor:
    softWrap: true
